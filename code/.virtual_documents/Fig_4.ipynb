@njit
def rhs(X, t, theta):
    # unpack parameters
    x, y, n = X
    r1, r2, b12, b21, x0, y0, n0 = theta
    # equations
    dn_dt = r1*x*n + r2*y*n + b12*x*y*n*n + b21*x*y*n*n
    dx_dt = (r1*x*n + b12*x*y*n*n - x*dn_dt)/n
    dy_dt = (r2*y*n + b21*x*y*n*n - y*dn_dt)/n
    
    return [dx_dt, dy_dt, dn_dt]

