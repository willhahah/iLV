# function that calculates residuals based on a given theta
def ode_model_resid(thetab):
    return (
        np.abs(df[["hare_rel", "lynx_rel"]] - np.delete(odeint(func=rhs, y0=thetab[-3:], t=time, args=(thetab,)), -1, axis=1))
    ).values.flatten()


